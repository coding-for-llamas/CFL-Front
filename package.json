  "license": "MIT",
  "scripts": {
    "start:dist": "node server.js",
    "build:prod": "rm -rf dist && yarn start build",
    "build:prod:windows": "rmdir /s /q dist && yarn start build",
    "cc": "codeclimate analyze -f html > codeclimate.html",
    "start": "nps",
    "test:lint-fix": "yarn test:stylelint && nps test.lint.fix",
    "test:lint": "yarn test:stylelint && nps test.lint",
    "test": "yarn test:lint && yarn test:jest",
    "test:cc": "yarn test:lint && yarn test:jest:cc",
    "test:local": "yarn test:lint-fix && yarn test:jest && yarn test:puppeteer && yarn cc",
    "test:jest": "jest --config jest.config.json --no-cache --runInBand",
    "test:jest:cc": "jest --config jest.config.json --no-cache --maxWorkers=2",
    "test:puppeteer": "jest --preset jest-puppeteer --config jest.puppeteer-config.json /test/puppeteer --runInBand",
    "test:puppeteer:cc": "jest --preset jest-puppeteer --config jest.puppeteer-config.json /test/puppeteer --maxWorkers=2",
    "installglobals": "yarn config set registry https://registry.yarnpkg.com -g",
    "cleaninstall": "rm -rf yarn.lock && rm -rf node_modules && yarn cache clean && yarn installglobals && yarn install",
    "cleaninstall:windows": "del /f /q yarn.lock && del /f /q node_modules && yarn cache clean && yarn installglobals && yarn install",
    "postinstall": "yarn start build",
    "test:stylelint": "stylelint \"static/**/*.scss\""
  },
